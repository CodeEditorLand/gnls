{
	"name": "gnls",
	"displayName": "GN Language Server",
	"version": "0.1.4",
	"description": "Code IntelliSense for GN build system.",
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"gn",
		"chromium"
	],
	"homepage": "https://github.com/microsoft/gnls",
	"bugs": {
		"url": "https://github.com/microsoft/gnls/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/gnls.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"publisher": "msedge-dev",
	"main": "./build/index.js",
	"scripts": {
		"build": "ts-node script build",
		"debug": "ts-node script debug",
		"format": "ts-node script format",
		"package": "ts-node script package",
		"prepare": "ts-node script prepare",
		"test": "ts-node script test"
	},
	"contributes": {
		"grammars": [
			{
				"language": "gn",
				"path": "./build/grammar.json",
				"scopeName": "source.gn"
			}
		],
		"languages": [
			{
				"aliases": [
					"GN",
					"gn"
				],
				"configuration": "./build/language.json",
				"extensions": [
					"gn",
					"gni"
				],
				"id": "gn"
			}
		]
	},
	"prettier": {
		"arrowParens": "always",
		"bracketSameLine": true,
		"bracketSpacing": false,
		"jsxSingleQuote": true,
		"printWidth": 120,
		"quoteProps": "consistent",
		"semi": false,
		"singleQuote": true,
		"tabWidth": 2,
		"trailingComma": "all",
		"useTabs": false
	},
	"eslintConfig": {
		"parser": "@typescript-eslint/parser",
		"parserOptions": {
			"project": true,
			"tsconfigRootDir": "."
		},
		"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/recommended-type-checked",
			"plugin:@typescript-eslint/stylistic-type-checked"
		],
		"rules": {
			"no-undef": "off"
		},
		"root": true
	},
	"dependencies": {},
	"devDependencies": {
		"@codeeditorland/languageclient": "0.0.1",
		"@codeeditorland/languageserver": "0.0.1",
		"@codeeditorland/languageserver-textdocument": "0.0.1",
		"@codeeditorland/plugin-commonjs": "0.0.1",
		"@codeeditorland/plugin-node-resolve": "0.0.1",
		"@codeeditorland/plugin-terser": "0.0.1",
		"@codeeditorland/plugin-typescript": "0.0.1",
		"@codeeditorland/uri": "0.0.1",
		"@codeeditorland/vsce": "0.0.1",
		"@types/jest": "^29.5.7",
		"@types/node": "^20.8.10",
		"@types/vscode": "^1.83.2",
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.1",
		"eslint": "^8.52.0",
		"jest": "^29.7.0",
		"node-addon-api": "^7.0.0",
		"node-gyp": "^10.0.0",
		"prettier": "^3.0.3",
		"rollup": "^4.2.0",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"tslib": "^2.6.2",
		"typescript": "^5.0.4"
	},
	"engines": {
		"vscode": "^1.83.0"
	},
	"extensionKind": [
		"workspace"
	]
}
